{
  "name": "kkg-gugus3-wanayasa",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "wrangler pages dev dist",
    "deploy": "npm run build && wrangler pages deploy dist",
    "deploy:prod": "npm run build && wrangler pages deploy dist --project-name kkg-gugus3-wanayasa",
    "dev:sandbox": "wrangler pages dev dist --d1=kkg-production --local --ip 0.0.0.0 --port 8788",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "db:migrate:local": "wrangler d1 migrations apply kkg-production --local",
    "db:seed": "wrangler d1 execute kkg-production --local --file=./seed.sql",
    "db:reset": "rm -rf .wrangler/state/v3/d1 && npm run db:migrate:local && npm run db:seed",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@hono/vite-cloudflare-pages": "^0.4.3",
    "docx": "^9.5.1",
    "file-saver": "^2.0.5",
    "hono": "^4.7.5",
    "qrcode": "^1.5.4",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260207.0",
    "@hono/vite-build": "^1.2.0",
    "@hono/vite-dev-server": "^0.18.2",
    "@types/qrcode": "^1.5.6",
    "@vitest/coverage-v8": "^4.0.18",
    "happy-dom": "^20.5.0",
    "vite": "^6.3.5",
    "vitest": "^4.0.18",
    "wrangler": "^4.4.0"
  }
}
